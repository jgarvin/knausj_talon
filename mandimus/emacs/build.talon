os: linux
app: emacs
-

(build|billed) menu: key(ctrl-u ctrl-c b)
run menu: key(ctrl-u ctrl-c r)
(build|billed) only: key(ctrl-c b)
stale run: key(ctrl-c r)
stop run: key(ctrl-c s r)
stop (build|billed): key(ctrl-c s b)
stop run: key(ctrl-c s r)
execute: key(ctrl-c c)
new (build|billed) script: key(ctrl-c n b)
new run script: key(ctrl-c n r)
open (build|billed) script: key(ctrl-c o b)
open run script: key(ctrl-c o r)

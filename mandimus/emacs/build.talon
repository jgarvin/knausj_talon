os: linux
app: emacs
-

build menu: key(ctrl-u ctrl-c b)
run menu: key(ctrl-u ctrl-c r)
build only: key(ctrl-c b)
stale run: key(ctrl-c r)
stop run: key(ctrl-c s b)
execute: key(ctrl-c c)
new build script: key(ctrl-c n b)
new run script: key(ctrl-c n r)
open build script: key(ctrl-c b)
open run script: key(ctrl-c o r)
